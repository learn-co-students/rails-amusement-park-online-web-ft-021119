<h1>Sign Up</h1>

<%= form_for @user do |f| %>
<%= f.label :user_name %>
<%= f.text_field :name %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :height %>
<%= f.text_field :height %>
<%= f.label :happiness %>
<%= f.text_field :happiness %>
<%= f.label :nausea %>
<%= f.text_field :nausea %>
<%= f.label :tickets %>
<%= f.text_field :tickets %>
<%= f.label :user_admin %>
<%= f.check_box :admin %>
<%= f.submit %>


<% end %>
